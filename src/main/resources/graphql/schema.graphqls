type Note {
    id: ID!
    title: String!
    createdByUser: User!
    content: String!
}

type User {
    id: ID!
    uniqueId: String!
}


input NoteInput {
    title: String!
    author: String
}

type Query {
    notes(note: NoteInput): [Note]!
    noteById(id: ID!): Note
}

